{
  "name": "assembox",
  "repository": {
    "url": "https://github.com/cabin-in/webpages-packer"
  },
  "scripts": {
    "bootstrap": "pnpm i",
    "lint": "eslint --fix packages --ext .js --ext .jsx",
    "dev:core": "pnpm --filter assembox-core run dev",
    "build:core": "pnpm --filter assembox-core run build",
    "dev:desktop": "pnpm --filter assembox-desktop run dev",
    "build:desktop": "pnpm --filter assembox-desktop run build",
    "dev": "pnpm --filter vue-assembox-docs-website run dev",
    "build": "pnpm --filter vue-assembox-docs-website run build",
    "start": "pnpm --filter vue-assembox-docs-website run start",
    "dev:demo": "pnpm --filter desktop-demo run dev",
    "start:demo": "pnpm --filter desktop-demo run start"
  },
  "engines": {
    "node": ">=12"
  },
  "author": "danielmlc",
  "license": "ISC",
  "devDependencies": {
    "pnpm": "^10.12.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "typescript": "*"
  }
}
