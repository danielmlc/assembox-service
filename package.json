{
  "name": "aseembox-service",
  "version": "0.0.1",
  "description": "AseemBox - Enterprise SaaS Low-Code Platform Service",
  "author": "",
  "private": true,
  "scripts": {
    "cp:config": "cp ./config.yaml ./dist",
